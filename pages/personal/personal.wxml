<view class="personal">
  <view class='header' style="background-image:url('../../image/1.jpg')">
    <view class='person-info'>
      <image src="{{avatarUrl}}" class='avator' ></image>
      <view class='person-info-text'>
        <text>昵称：{{nikeName}}</text>
      </view>
    </view>
  </view>
  <view>
    <view class="personal-item" wx:for="{{setup_list}}" bindtap='{{item.method}}'>
      <text>{{item.text}}</text>
      <image src='../../image/left_arrow.png'></image>
    </view>
  </view>
</view>